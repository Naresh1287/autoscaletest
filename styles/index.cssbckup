  /* Tooltip container */
  .bottom-menu li {
    position: relative;
    display: inline-block;
  }
  .tooltiptext {
    width: 90%;
    background-color: #FFFFFF;
    color: #000;
    line-height: 20px;
    border-radius: 3px;
    position: fixed;
    bottom: 0;
    left: 0;
    display: none;
    text-align: justify;
    overflow-x: hidden;
    margin-left: -10px;
    height: 90%;
    font-size: 12px;
    padding: 20px;
    padding-top: 20px;
    margin: 0 auto;
    right: 0;
    z-index: 99999999;
    top: 50%;
    transform: translateY(-50%);
  }
  .tooltiptext div {
    margin-top: 15px;
    position: absolute;
    left: 0;
    right: 0;
    padding: 20px;
    overflow-x:hidden;
    overflow-y: auto;
    height: 80vh;
  }
  span.tooltiptext.addresspopup div {
    top: 15%;
    height: auto;
  }
  span.tooltiptext.addresspopup {
    height: 40vh;
    font-size: 17px;
    line-height: 25px;
  }
  .tooltiptext span.closeicon {
    font-size: 30px;
    top: 10px;
    position: fixed;
    right: 15px;
  }
   /* !important is needed sometimes */
 ::-webkit-scrollbar {
    width: 8px !important;
 }

 /* Track */
::-webkit-scrollbar-track {
   -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3) !important;
   -webkit-border-radius: 10px !important;
   border-radius: 10px !important;
 }

 /* Handle */
 ::-webkit-scrollbar-thumb {
   -webkit-border-radius: 10px !important;
   border-radius: 10px !important;
   background: #41617D !important; 
   -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5) !important; 

 }
 ::-webkit-scrollbar-thumb:window-inactive {
   background: #41617D !important; 
 }
