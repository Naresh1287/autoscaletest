/*preloader*/
#preloader {
  padding:0;
  margin:0;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999999;
  background-color:#FFFFFF;
}
#preloader p {
  text-align: center;
  width: 100%;
  font-weight: 500;
  margin: 0px auto;
  position: absolute;
  top:45%;
  font-size: 30px;
  color: #000;
  font-family: 'Futuralight',sans-serif;
  transform:translateY(-45%);
  -webkit-transform:translateY(-45%);
}
#preloader p img {
  width:350px;
  height:117px;
  position:relative;
  bottom:30px;
  max-width:100%;
}
.preloader {
  width: 100%;
  margin: 0px auto;
  position: absolute;
  top:50%;
  transform:translateY(-50%);
  -webkit-transform:translateY(-50%);
}
.preloader img {
  width: 80%;
}
@media only screen and (min-width: 600px){
  .freshon-container .scroll-target.section-one .logo img {
    width: 75%;
  }
  #preloader p img {
    width: 500px;
    height: auto;
    position: relative;
    bottom: 110px;
    max-width: 100%;
  }
  .freshon-container .scroll-target.section-seven ul.bottom-menu li {
    font-size: 25px;
    padding: 10px;
  }
  .bottom-menu li .tooltiptext {
    width: 230px;
    bottom: 100%;
    height: 100px;
  }
  .bottom-menu li .tooltiptext::after {
    content: " ";
    position: absolute;
    top: 100%;
    left: 4%;
    border-width: 12px;
    border-style: solid;
    border-color: #FFFFFF transparent transparent transparent;
  }
  .bottom-menu li span.closeicon {
    position: absolute;
    right: 10px;
    top: -1px;
    font-size: 30px;
    font-weight: bold;
  }
  .bottom-menu li.terms .tooltiptext {
    width: 200px;
    bottom: 100%;
    height: 110px;
  }
  .preloader b {
    font-size: 30px !important;
  }
  .bottom-menu li .tooltiptext {
    width: 200px;
    height: 90px;
  }
}
@media only screen and (min-width: 550px) and (orientation:landscape) {
  .freshon-container .scroll-target.section-one .trucklogo img.truck {
    width: 150px;
  }
  .preloader {
    width: 100%;
    margin: 0px auto;
  }
  .freshon-container .preloader video {
    width: 60%;
  }
  .freshon-container .preloader img {
    width: 50%;
  }
  .freshon-container .scroll-target.section-six .order-time .ordernow {
    width: 98%;
  }
  .freshon-container .scroll-target.section-six .order-time {
    height: 70vh;
  }
  .freshon-container .scroll-target.section-six .title-image {
    height: 30vh;
  }
}
