* {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
html, body {
  padding: 0;
  margin: 0;
  width: 100%;
}
@font-face {
  font-family: Futuralight;
  src: url(../fonts/Futura-Std-Light.ttf);
}
@font-face {
  font-family: Futuralightcondensed;
  src: url(../fonts/Futura-Std-Light-Condensed.ttf);
}
@font-face {
  font-family: Futuramedium;
  src: url(../fonts/Futura-Std-Medium.ttf);
}
@font-face {
  font-family: Futuraheavy;
  src: url(../fonts/Futura-Std-Heavy.ttf);
}

video::-webkit-media-controls {
  display: none;
}

.freshon-container .preloader video {
  width: 100%;
}

.freshon-container .arrow-bottom {
  width: 40px;
  height: 40px;
  line-height: 43px;
  text-align: center;
  font-size: 35px;
  position: fixed;
  bottom: -40px;
  border-radius: 50%;
  right: 20px;
  -webkit-transition: all 1s ease;
  -moz-transition: all 1s ease;
  -o-transition: all 1s ease;
  transition: all 1s ease;
  background-color: #4CAF50;
  color: #FFFFFF;
  text-decoration: none;
  background: #4CAF50;
  cursor: pointer;
  z-index: 99999;
}
.freshon-container span.arrow-bottom.arrow-up.show {
  line-height: 35px;
}
.freshon-container .arrow-bottom.show  {
  bottom: 15px;
}
.freshon-container .arrow-bottom:hover
{
  color: #FFFFFF;
}
.freshon-container .spaces {
  height: 15px;
}

a {
  color: #000;
  text-decoration: none;
}
a:hover,
a:active,
a:focus {
  color: #000;
  text-decoration: none;
}
